{
  "name" : "APIM Api Latency",
  "id" : "APIMApiLatency",
  "thumbnailURL" : "",
  "configs" : {
    "pubsub" : {
      "types" : ["subscriber"]
    },
    "providerConfig" : {
      "configs" : {
        "type" : "RDBMSStreamingDataProvider",
        "config" : {
          "datasourceName": "APIM_ANALYTICS_DB",
          "queryData" : {
            "latencyquery": "select apiName, MAX(AGG_SUM_backendLAtency) as latency from ApiExeTime_HOURS WHERE AGG_TIMESTAMP between '{{From}}' and '{{To}}' group by apiName"
          },
          "tableName": "ApiExeTime_HOURS",
          "incrementalColumn": "AGG_SUM_backendLAtency",
          "publishingLimit": 2147483647,
          "purgingInterval": 6,
          "purgingLimit": 6,
          "isPurgingEnable": true
        }
      }
    },
    "options": [
      {
        "id" : "header",
        "title" : "Header",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": true
      }
    ]
  }
}